<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="4755ad61-664a-40d3-9831-b8bf2fe21ea0" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/2017.2/project-config.xsd">
  <name>Build</name>
  <description />
  <settings>
    <options>
      <option name="allowExternalStatus" value="true" />
      <option name="allowPersonalBuildTriggering" value="false" />
      <option name="artifactRules" value="SeleniumBasics\bin\Debug\log4net.dll =&gt; bin&#xA;SeleniumBasics\bin\Debug\nunit.framework.dll =&gt; bin&#xA;SeleniumBasics\bin\Debug\SeleniumBasics.dll =&gt; bin&#xA;SeleniumBasics\bin\Debug\Shouldly.dll =&gt; bin&#xA;SeleniumBasics\bin\Debug\WebDriver.dll =&gt; bin&#xA;SeleniumBasics\Logs\*.log =&gt; Logs&#xA;packages\NUnit.ConsoleRunner.3.7.0\tools\nunit3-console.exe =&gt; bin&#xA;packages\ReportUnit.1.2.1\tools\ReportUnit.exe =&gt; bin&#xA;SeleniumBasics\Reports\*.xml =&gt; Reports\xml&#xA;SeleniumBasics\Reports\*.html =&gt; Reports" />
      <option name="buildConfigurationType" value="DEPLOYMENT" />
      <option name="buildNumberPattern" value="0.0.0.%build.counter%" />
      <option name="cleanBuild" value="true" />
      <option name="maximumNumberOfBuilds" value="1" />
      <option name="showDependenciesChanges" value="true" />
    </options>
    <disabled-settings>
      <setting-ref ref="ARTIFACT_DEPENDENCY_1" />
      <setting-ref ref="BUILD_EXT_1" />
    </disabled-settings>
    <parameters>
      <param name="env.BuildNumber" value="build-v%build.number%" />
      <param name="env.EnableNuGetPackageRestore" value="true" />
      <param name="env.NUnitReportXMLs" value="%teamcity.build.workingDir%\SeleniumBasics\Reports" />
      <param name="env.ReportUnitPath" value="%teamcity.agent.work.dir%\packages\ReportUnit.1.2.1\tools\ReportUnit.exe" />
      <param name="env.VisualStudioVersion" value="15.0" />
    </parameters>
    <build-runners order="RUNNER_1, RUNNER_2, RUNNER_3, RUNNER_4, RUNNER_5, RUNNER_6">
      <runner id="RUNNER_1" name="" type="VS.Solution">
        <parameters>
          <param name="build-file-path" value="SeleniumBasics.sln" />
          <param name="msbuild.prop.Configuration" value="Debug" />
          <param name="msbuild_version" value="15.0" />
          <param name="run-platform" value="x86" />
          <param name="targets" value="Rebuild" />
          <param name="teamcity.step.mode" value="default" />
          <param name="toolsVersion" value="15.0" />
          <param name="vs.version" value="vs2017" />
        </parameters>
      </runner>
      <runner id="RUNNER_2" name="" type="dotnet.cli">
        <parameters>
          <param name="command" value="restore" />
          <param name="paths" value="SeleniumBasics.sln" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_3" name="" type="dotnet.cli">
        <parameters>
          <param name="command" value="build" />
          <param name="paths" value="SeleniumBasics.sln" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_4" name="Run Tests" type="NUnit">
        <parameters>
          <param name="dotNetCoverage.NCover.HTMLReport.File.Sort" value="0" />
          <param name="dotNetCoverage.NCover.HTMLReport.File.Type" value="1" />
          <param name="dotNetCoverage.NCover.Reg" value="selected" />
          <param name="dotNetCoverage.NCover.platformBitness" value="x86" />
          <param name="dotNetCoverage.NCover.platformVersion" value="v2.0" />
          <param name="dotNetCoverage.NCover3.Reg" value="selected" />
          <param name="dotNetCoverage.NCover3.args" value="//ias .*" />
          <param name="dotNetCoverage.NCover3.platformBitness" value="x86" />
          <param name="dotNetCoverage.NCover3.platformVersion" value="v2.0" />
          <param name="dotNetCoverage.NCover3.reporter.executable.args" value="//or FullCoverageReport:Html:{teamcity.report.path}" />
          <param name="dotNetCoverage.PartCover.Reg" value="selected" />
          <param name="dotNetCoverage.PartCover.executable" value="%system.teamcity.build.workingDir%\" />
          <param name="dotNetCoverage.PartCover.includes" value="[*]*" />
          <param name="dotNetCoverage.PartCover.platformBitness" value="NATIVE" />
          <param name="dotNetCoverage.PartCover.platformVersion" value="v4.0" />
          <param name="dotNetCoverage.dotCover.home.path" value="%teamcity.tool.JetBrains.dotCover.CommandLineTools.DEFAULT%" />
          <param name="dotNetTestRunner.Type" value="NUnit" />
          <param name="nunit_command_line" value="--result:%env.NUnitReportXMLs%\%env.BuildNumber%.xml" />
          <param name="nunit_enabled" value="checked" />
          <param name="nunit_environment" value="auto" />
          <param name="nunit_include" value="SeleniumBasics\bin\Debug\SeleniumBasics.dll" />
          <param name="nunit_path" value="%teamcity.tool.NUnit.Console.DEFAULT%" />
          <param name="nunit_platform" value="MSIL" />
          <param name="nunit_version" value="NUnit-3.0.0" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_5" name="Report Generation" type="simpleRunner">
        <parameters>
          <param name="script.content" value="'%env.ReportUnitPath%' '%env.NUnitReportXMLs%'" />
          <param name="teamcity.build.workingDir" value="SeleniumBasics/Reports" />
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="SeleniumBasics_HttpsGithubComDeLabsSeleniumBasicsGitRefsHeadsMaster" />
    </vcs-settings>
    <requirements>
      <exists id="RQ_1" name="system.teamcity.dotnet.nunitlauncher" />
    </requirements>
    <build-triggers>
      <build-trigger id="vcsTrigger" type="vcsTrigger">
        <parameters>
          <param name="enableQueueOptimization" value="true" />
          <param name="groupCheckinsByCommitter" value="true" />
          <param name="quietPeriodMode" value="DO_NOT_USE" />
        </parameters>
      </build-trigger>
    </build-triggers>
    <build-extensions>
      <extension id="BUILD_EXT_1" type="xml-report-plugin">
        <parameters>
          <param name="xmlReportParsing.reportDirs" value="%teamcity.agent.home.dir%\temp\buildTmp\*.xml" />
          <param name="xmlReportParsing.reportType" value="nunit" />
        </parameters>
      </extension>
      <extension id="BUILD_EXT_2" type="VcsLabeling">
        <parameters>
          <param name="labelingPattern" value="%env.BuildNumber%" />
          <param name="successfulOnly" value="true" />
          <param name="vcsRootId" value="__ALL__" />
        </parameters>
      </extension>
      <extension id="JetBrains.AssemblyInfo" type="JetBrains.AssemblyInfo">
        <parameters>
          <param name="assembly-format" value="%build.number%" />
          <param name="patch-global-assembly-info" value="true" />
        </parameters>
      </extension>
      <extension id="perfmon" type="perfmon">
        <parameters />
      </extension>
      <extension id="BUILD_EXT_3" type="commit-status-publisher">
        <parameters>
          <param name="github_authentication_type" value="token" />
          <param name="github_host" value="https://api.github.com" />
          <param name="publisherId" value="githubStatusPublisher" />
          <param name="secure:github_access_token" value="credentialsJSON:e72a5ffc-3824-4a59-a369-1a51febfd1c1" />
          <param name="vcsRootId" value="SeleniumBasics_HttpsGithubComDeLabsSeleniumBasicsGitRefsHeadsMaster" />
        </parameters>
      </extension>
    </build-extensions>
    <artifact-dependencies>
      <dependency id="ARTIFACT_DEPENDENCY_1" sourceBuildTypeId="SeleniumBasics_Build" cleanDestination="false">
        <revisionRule name="lastSuccessful" revision="latest.lastSuccessful" />
        <artifact sourcePath="bin\xml\*.xml =&gt; bin\xml&#xD;&#xA;Logs\*.log =&gt; Logs" />
      </dependency>
    </artifact-dependencies>
    <cleanup />
  </settings>
</build-type>

